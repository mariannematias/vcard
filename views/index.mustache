
<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>{{global.name}}</title>

        <link href="css/bootstrap.min.css" rel="stylesheet">
        <link href="css/style.css" rel="stylesheet">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    </head>

    <body>

        <main>
            <section class="main">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-12 left">
                            <div class="profile">
                                <p><img src="{{page.profile.img}}"></p>
                            </div>
                            <ul class="nav">
                                {{#page.nav}}
                                    <li class="{{class}}"><a href="{{href}}">{{text}}<i class="{{icon}}"></i></a></li>
                                {{/page.nav}}
                            </ul>
                        </div>
                        <div class="col-md-9 col-sm-12 right">
                            <div class="body">
                                <div id="profile" class="profile-section">
                                    <h2>{{page.profile.content.title}} <strong>{{page.profile.content.name}}</strong><br>{{page.profile.content.position}}</h2>
                                    <p class="line"></p>
                                    <p class="desc">{{page.profile.content.desc}}</p>
                                    <div class="info">
                                        <h4>{{page.profile.content.info.title}}</h4>
                                        <p class="line2"></p>
                                    </div>
                                </div>
                                
                            </div>

                            <!-- <footer>
                                <div class="container">
                                    <div class="footer">
                                        <p class="copyright">{{page.footer.copyright}}</p>
                                    </div>
                                </div>
                            </footer> -->
                        </div>
                    </div>
                </div>
            </section>
        </main>
        
    </body>

    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/custom.js"></script>
  </body>
</html>
